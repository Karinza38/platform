<% layout("./layout") %>

<div x-data="page">
  <ul>
    <template x-for="instanceId in instanceIds">
      <li>
        <a :href="`/configure?instanceId=${instanceId}`" x-text="instanceId"></a>
      </li>
    </template>
  </ul>
</div>

<script>
	document.addEventListener("alpine:init", () => {
		Alpine.data("page", () => ({
			instanceIds: <%~ JSON.stringify(it.instanceIds) %>,
		}))
	})
</script>