{
	"name": "@pubpub/integration-sdk",
	"type": "module",
	"version": "0.0.5",
	"description": "",
	"main": "dist/index.js",
	"files": [
		"dist",
		"style.css"
	],
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./style.css": "./css/style.css"
	},
	"scripts": {
		"dev": "vite",
		"build": "pnpm build:lib && pnpm build:css",
		"build:lib": "tsup src/lib/index.ts --format esm,cjs --dts",
		"build:css": "tsup src/css/style.css --outDir . --minify",
		"prepare": "pnpm build"
	},
	"keywords": [
		"pubpub",
		"integration",
		"sdk"
	],
	"author": "Knowledge Futures, Inc <hello@knowledgefutures.org>",
	"license": "GPL-2.0+",
	"devDependencies": {
		"@types/node": "^20.4.2",
		"autoprefixer": "^10.4.14",
		"postcss": "^8.4.26",
		"postcss-import": "^15.1.0",
		"postcss-inline-svg": "^6.0.0",
		"postcss-minify": "^1.1.0",
		"postcss-mixins": "^9.0.4",
		"postcss-nested": "^6.0.1",
		"postcss-simple-vars": "^7.0.1",
		"tsup": "^7.1.0",
		"vite": "^4.4.5"
	}
}
